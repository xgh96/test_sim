<?xml version="1.0" ?>
<robot name="arm6dof">
  <material name="Grey"> <color rgba="0.7 0.7 0.7 1.0"/> </material>
  <material name="Blue"> <color rgba="0.1 0.4 0.8 1.0"/> </material>

  <link name="base_link">
    <visual> <geometry> <cylinder length="0.05" radius="0.06"/> </geometry> <material name="Grey"/> </visual>
    <collision> <geometry> <cylinder length="0.05" radius="0.06"/> </geometry> </collision>
    <inertial> <mass value="2.0"/> <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01"/> </inertial>
  </link>

  <joint name="shoulder_pan_joint" type="revolute">
    <parent link="base_link"/> <child link="shoulder_link"/>
    <origin xyz="0 0 0.05"/> <axis xyz="0 0 1"/>
    <limit effort="330" lower="-6.28" upper="6.28" velocity="2.16"/>
  </joint>

  <link name="shoulder_link">
    <visual> <geometry> <sphere radius="0.05"/> </geometry> <material name="Blue"/> </visual>
    <collision> <geometry> <sphere radius="0.05"/> </geometry> </collision>
    <inertial> <mass value="2.0"/> <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01"/> </inertial>
  </link>

  <joint name="shoulder_lift_joint" type="revolute">
    <parent link="shoulder_link"/> <child link="upper_arm_link"/>
    <origin xyz="0 0.1 0.05"/> <axis xyz="0 1 0"/>
    <limit effort="330" lower="-6.28" upper="6.28" velocity="2.16"/>
  </joint>

  <link name="upper_arm_link">
    <visual> <origin xyz="0 0 0.15"/> <geometry> <cylinder length="0.30" radius="0.04"/> </geometry> <material name="Grey"/> </visual>
    <collision> <origin xyz="0 0 0.15"/> <geometry> <cylinder length="0.30" radius="0.04"/> </geometry> </collision>
    <inertial> <mass value="3.42"/> <inertia ixx="0.02" ixy="0" ixz="0" iyy="0.02" iyz="0" izz="0.02"/> </inertial>
  </link>

  <joint name="elbow_joint" type="revolute">
    <parent link="upper_arm_link"/> <child link="forearm_link"/>
    <origin xyz="0 -0.05 0.30"/> <axis xyz="0 1 0"/>
    <limit effort="150" lower="-3.14" upper="3.14" velocity="3.15"/>
  </joint>

  <link name="forearm_link">
    <visual> <origin xyz="0 0 0.125"/> <geometry> <cylinder length="0.25" radius="0.035"/> </geometry> <material name="Blue"/> </visual>
    <collision> <origin xyz="0 0 0.125"/> <geometry> <cylinder length="0.25" radius="0.035"/> </geometry> </collision>
    <inertial> <mass value="1.26"/> <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01"/> </inertial>
  </link>

  <joint name="wrist_1_joint" type="revolute">
    <parent link="forearm_link"/> <child link="wrist1_link"/>
    <origin xyz="0 0 0.25"/> <axis xyz="0 1 0"/>
    <limit effort="120" lower="-6.28" upper="6.28" velocity="3.2"/>
  </joint>

  <link name="wrist1_link">
    <visual> <origin xyz="0 0 0.025"/> <geometry> <cylinder length="0.05" radius="0.03"/> </geometry> <material name="Grey"/> </visual>
    <collision> <origin xyz="0 0 0.025"/> <geometry> <cylinder length="0.05" radius="0.03"/> </geometry> </collision>
    <inertial> <mass value="0.6"/> <inertia ixx="0.002" ixy="0" ixz="0" iyy="0.002" iyz="0" izz="0.002"/> </inertial>
  </link>

  <joint name="wrist_2_joint" type="revolute">
    <parent link="wrist1_link"/> <child link="wrist2_link"/>
    <origin xyz="0 0 0.05"/> <axis xyz="1 0 0"/>
    <limit effort="80" lower="-6.28" upper="6.28" velocity="3.2"/>
  </joint>

  <link name="wrist2_link">
    <visual> <origin xyz="0 0 0.025"/> <geometry> <cylinder length="0.05" radius="0.028"/> </geometry> <material name="Blue"/> </visual>
    <collision> <origin xyz="0 0 0.025"/> <geometry> <cylinder length="0.05" radius="0.028"/> </geometry> </collision>
    <inertial> <mass value="0.5"/> <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/> </inertial>
  </link>

  <joint name="wrist_3_joint" type="revolute">
    <parent link="wrist2_link"/> <child link="wrist3_link"/>
    <origin xyz="0 0 0.05"/> <axis xyz="0 1 0"/>
    <limit effort="60" lower="-6.28" upper="6.28" velocity="3.2"/>
  </joint>

  <link name="wrist3_link">
    <visual> <origin xyz="0 0 0.025"/> <geometry> <cylinder length="0.05" radius="0.026"/> </geometry> <material name="Grey"/> </visual>
    <collision> <origin xyz="0 0 0.025"/> <geometry> <cylinder length="0.05" radius="0.026"/> </geometry> </collision>
    <inertial> <mass value="0.4"/> <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/> </inertial>
  </link>

  <joint name="gripper_base_joint" type="fixed">
    <parent link="wrist3_link"/> <child link="gripper_base"/>
    <origin xyz="0 0 0.07"/>
  </joint>

  <link name="gripper_base">
    <visual> <geometry> <box size="0.06 0.04 0.04"/> </geometry> <material name="Grey"/> </visual>
    <collision> <geometry> <box size="0.06 0.04 0.04"/> </geometry> </collision>
    <inertial> <mass value="0.3"/> <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/> </inertial>
  </link>

  <joint name="tool_tip_joint" type="fixed">
    <parent link="gripper_base"/> <child link="tool_tip"/>
    <origin xyz="0 0 0.04"/>
  </joint>

  <link name="tool_tip"/>

  <joint name="f1" type="prismatic"> <parent link="gripper_base"/> <child link="l_finger"/> <axis xyz="0 1 0"/> <limit effort="50" lower="-0.04" upper="0"/> </joint>
  <link name="l_finger"> <visual> <origin xyz="0 0.03 0.04"/> <geometry> <box size="0.01 0.01 0.06"/> </geometry> </visual> <collision> <origin xyz="0 0.03 0.04"/> <geometry> <box size="0.01 0.01 0.06"/> </geometry> </collision> </link>
  <joint name="f2" type="prismatic"> <parent link="gripper_base"/> <child link="r_finger"/> <axis xyz="0 1 0"/> <limit effort="50" lower="0" upper="0.04"/> </joint>
  <link name="r_finger"> <visual> <origin xyz="0 -0.03 0.04"/> <geometry> <box size="0.01 0.01 0.06"/> </geometry> </visual> <collision> <origin xyz="0 -0.03 0.04"/> <geometry> <box size="0.01 0.01 0.06"/> </geometry> </collision> </link>
</robot>
